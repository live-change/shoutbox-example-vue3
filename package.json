{
  "name": "shoutbox-example-e2e",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "memDev": "../cli/bin/lcli.js ssrDev --ssrRoot front --withApi --withServices --updateServices --services server/services.config.js --withDb --dbBackend mem --createDb",
    "dev": "../cli/bin/lcli.js ssrDev --ssrRoot front --withApi --withServices --updateServices --services server/services.config.js",  
    "ssrDev": "lcli ssrDev",
    "serveAll": "cross-env NODE_ENV=production lcli ssrServer --ssrRoot front --withApi --withServices --updateServices --services server/services.config.js",
    "serve": "cross-env NODE_ENV=production lcli ssrServer --ssrRoot front",
    "apiServer": "lcli apiServer --withServices --updateServices --services server/services.config.js",
    "memApiServer": "lcli apiServer --withServices --updateServices --services server/services.config.js --withDb --dbBackend mem --createDb",
    "build": "cd front; yarn build:client && yarn build:server",
    "build:client": "cd front; vite build --ssrManifest --outDir dist/client",
    "build:server": "cd front; vite build --ssr src/entry-server.js --outDir dist/server",
    "generate": "vite build --ssrManifest --outDir dist/static && yarn build:server && node prerender",    
    "debug": "node --inspect-brk server"
  },
  "dependencies": {
    "@live-change/cli": "^0.1.0",
    "@live-change/dao": "^0.2.44",
    "@live-change/dao-vue3": "^0.1.12",
    "@live-change/dao-websocket": "^0.2.21",
    "@live-change/vue3-components": "^0.1.0",
    "@live-change/vue-api": "^0.1.0",  
    "@vitejs/plugin-vue": "^1.0.0",
    "@vitejs/plugin-vue-jsx": "^1.1.2",
    "@vue/compiler-sfc": "^3.0.8",
    "@vue/server-renderer": "^3.0.6",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",    
    "rollup-plugin-node-builtins": "^2.1.2",
    "serialize-javascript": "^5.0.1",
    "serve-static": "^1.14.1",
    "vite": "^2.3.0",
    "vue": "^3.0.8",
    "vue-router": "^4.0.3",
    "vue-meta": "^3.0.0-alpha.3"
  },
  "devDependencies": {
    "playwright": "^1.11.1",
    "webdriverio": "^7.7.2",
    "@wdio/selenium-standalone-service": "^7.7.0",
    "codeceptjs": "^3.0.7",
    "txtgen": "^2.2.8"
  },
  "author": "",
  "license": "ISC",
  "description": ""
}
