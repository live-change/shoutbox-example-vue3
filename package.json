{
  "name": "shoutbox-example",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "memDev": "lcli ssrDev --ssrRoot front --withApi --withServices --updateServices --services server/services.config.js --withDb --dbBackend mem --createDb",
    "dev": "lcli ssrDev --ssrRoot front --withApi --withServices --updateServices --services server/services.config.js",
    "ssrDev": "lcli ssrDev",
    "serveAll": "cross-env NODE_ENV=production lcli ssrServer --ssrRoot front --withApi --withServices --updateServices --services server/services.config.js",
    "serve": "cross-env NODE_ENV=production lcli ssrServer --ssrRoot front",
    "apiServer": "lcli apiServer --withServices --updateServices --services server/services.config.js",
    "memApiServer": "lcli apiServer --withServices --updateServices --services server/services.config.js --withDb --dbBackend mem --createDb",
    "build": "cd front; yarn build:client && yarn build:server",
    "build:client": "cd front; vite build --ssrManifest --outDir dist/client",
    "build:server": "cd front; vite build --ssr src/entry-server.js --outDir dist/server",
    "generate": "vite build --ssrManifest --outDir dist/static && yarn build:server && node prerender",
    "debug": "node --inspect-brk server"
  },
  "dependencies": {
    "@live-change/cli": "^0.1.4",
    "@live-change/dao": "^0.3.3",
    "@live-change/dao-vue3": "^0.1.14",
    "@live-change/dao-websocket": "^0.3.1",
    "@live-change/vue-api": "^0.1.2",
    "@live-change/vue3-components": "^0.1.2",
    "@vitejs/plugin-vue": "^1.2.3",
    "@vitejs/plugin-vue-jsx": "^1.1.5",
    "@vue/compiler-sfc": "^3.1.2",
    "@vue/server-renderer": "^3.1.2",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "rollup-plugin-node-builtins": "^2.1.2",
    "serialize-javascript": "^6.0.0",
    "serve-static": "^1.14.1",
    "vite": "^2.3.8",
    "vue": "^3.0.8",
    "vue-meta": "^3.0.0-alpha.3",
    "vue-router": "4.0.3"
  },
  "devDependencies": {
    "@live-change/framework": "^0.4.35",
    "@live-change/vue3-ssr": "^0.1.1",
    "@wdio/selenium-standalone-service": "^7.7.4",
    "codeceptjs": "^3.0.7",
    "playwright": "^1.12.2",
    "txtgen": "^2.2.8",
    "webdriverio": "^7.7.4"
  },
  "author": "",
  "license": "ISC",
  "description": ""
}
